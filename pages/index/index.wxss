.index_swiper swiper {
  height: 340rpx;
  width: 750rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .index_floor_item .item_title image {
  padding: 10rpx;
}
.index_floor .index_floor_item .item_list {
  overflow: hidden;
}
.index_floor .index_floor_item .item_list navigator {
  float: left;
  width: 33%;
}
.index_floor .index_floor_item .item_list navigator:nth-last-child(-n+4) {
  height: calc(33vw * 386 / 232 / 2);
  border-left: 2rpx solid #fff;
}
.index_floor .index_floor_item .item_list navigator:nth-last-child(2) {
  border-bottom: 2rpx solid #fff;
}
.index_floor .index_floor_item .item_list navigator:nth-last-child(3) {
  border-bottom: 2px solid #fff;
}
.index_floor .index_floor_item .item_list navigator image {
  width: 100%;
  height: 100%;
}
